{
  "name": "Startup Engine",
  "description": "An all-in-one platform for launching a startup.",
  "keywords": [
    "Startup",
    "CMS",
    "CRM",
    "Analytics",
    "Landing Page",
    "A/B Testing",
    "Subscriptions",
    "E-Commerce",
    "Server Automation",
    "Marketing Automation",
    "Dataviz",
    "Documentation",
    "Realtime",
    "Platform",
    "Framework",
    "JSON API",
    "GraphQL",
    "Markdown",
    "Bootstrap",
    "Vue",
    "Laravel",
    "Pusher",
    "Heroku",
    "Open-Source"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/M165437/drafter-php"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-gd": "*",
    "ext-pcntl": "*",
    "altek/accountant": "^1.1",
    "appstract/laravel-meta": "^0.2.0",
    "barryvdh/laravel-ide-helper": "^2.4",
    "beyondcode/laravel-websockets": "^1.0",
    "caffeinated/modules": "^4.2",
    "codewizz/laravel-reddit-api": "^0.1.2",
    "doctrine/dbal": "v2.4.2",
    "fico7489/laravel-eloquent-join": "^4.0",
    "fideloper/proxy": "~4.0",
    "folklore/graphql": "~1.0.0",
    "graham-campbell/digitalocean": "^5.1",
    "graham-campbell/markdown": "^10.0",
    "guzzlehttp/guzzle": "^6.3",
    "hashids/hashids": "^3.0",
    "hmaus/drafter-installer": "^1.0",
    "hmaus/drafter-php": "^5.0",
    "intervention/image": "^2.4",
    "jaam/mixpanel-data-export-api": "^0.1.2",
    "laracasts/utilities": "^3.0",
    "laravel/cashier": "~7.0",
    "laravel/framework": "5.7.7",
    "laravel/horizon": "^2.0",
    "laravel/passport": "^7.0",
    "laravel/socialite": "^3.0",
    "laravel/tinker": "~1.0",
    "laravolt/avatar": "^2.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "m165437/laravel-blueprint-docs": "^0.5.0",
    "nexuspoint/laravel-versioned": "1.0.2",
    "overtrue/laravel-follow": "^1.1",
    "owen-it/laravel-auditing": "^4.1",
    "phlak/semver": "^2.0",
    "php-http/guzzle6-adapter": "^1.1",
    "rtconner/laravel-tagging": "~2.2",
    "sfelix-martins/json-exception-handler": "^1.8",
    "shipping-docker/vessel": "^3.0",
    "spatie/laravel-json-api-paginate": "^1.4",
    "spatie/laravel-permission": "^2.7",
    "spatie/laravel-searchable": "^1.2",
    "symfony/property-access": "^4.2",
    "wikimedia/composer-merge-plugin": "^1.4",
    "laravel/telescope": "*"
  },
  "require-dev": {
    "barryvdh/laravel-httpcache": "*",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "orangehill/iseed": "^2.5",
    "phpunit/phpunit": "~7.0",
    "xethron/migrations-generator": "^2.0",
    "zetaphor/reddit-api-client": "dev-master"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Helpers/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "merge-plugin": {
      "include": [
        "app/Modules/*/composer.json"
      ]
    },
    "drafter-installer-tag": "v3.2.7"
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php artisan key:generate",
      "@php artisan migrate:fresh --seed",
      "@php artisan passport:install",
      "@php artisan passport:keys"
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate",
      "@php artisan migrate:fresh --seed"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-install-cmd": [
      "php artisan clear-compiled",
      "chmod -R 777 storage",
      "php artisan passport:keys"
    ],
    "post-update-cmd": [],
    "install-drafter": "Hmaus\\Drafter\\Installer::installDrafter"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "ext-pcntl": "7.2",
      "ext-posix": "7.2"
    }
  },
  "dependencies": {
    "minimatch": ">=3.0.2",
    "lodash": ">=4.17.5"
  }
}
